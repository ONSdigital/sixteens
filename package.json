{
  "name": "sixteens",
  "version": "0.0.1",
  "description": "Pattern library for ons website",
  "main": "",
  "scripts": {
    "clean": "",
    "build-css": "mkdir -pv dist/css  && ./node_modules/npm-sass/bin/npm-sass ./scss/main.scss > ./dist/css/main.css",
    "build-old-ie-css": "mkdir -pv dist/css  && ./node_modules/npm-sass/bin/npm-sass ./scss/old-ie.scss > ./dist/css/old-ie.css",
    "postinstall": "npm run build-css && npm run build-old-ie-css"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ONSdigital/sixteens.git"
  },
  "license": "Open Government License v2.0",
  "dependencies": {
    "npm-sass": "latest"
  }
}
